<ion-view view-title="Booking">
  <ion-content ng-controller="bookingCtrl" class="booking-page">
  	<div class="card booking-box">
	  <div class="item item-divider booking-box">
	   <i class="ion-earth" style="color:blue;"></i> City
	  </div>
	  <div class="item-text-wrap booking-box">
	   	<div class="item-input-inset booking-box">
		  <label class="item-input-wrapper booking-box" style="color:#fff;">
		    <i class="icon ion-ios-search placeholder-icon"></i>
		    <input ion-autocomplete type="text" class="ion-autocomplete booking-box-text" autocomplete="off" placeholder="Enter City Name"
               ng-model="model"
               item-value-key="name"
               item-view-value-key="view"
               items-method="getTestItems(query)"
               items-method-value-key="items"
               placeholder="Enter test query ..."
               items-clicked-method="itemsClicked(callback)"
               items-removed-method="itemsRemoved(callback)"
               max-selected-items="3"/>
		  </label>
		  <button class="button button-clear">
		   <i class="ion-backspace"></i>
		  </button>
		</div>
	  </div>

	<div class="item-divider booking-box" style="padding-left:16px;">
	   <i class="ion-home" style="color:#1E90FF;"></i> Pick Up Point
	</div>
	<div class="item item-text-wrap booking-box">
		<div class="item-input-inset booking-box">
		   <i class="ion-edit"></i>
		  <label class="item-input-wrapper booking-box" style="color:#fff;">
		    <input ion-autocomplete type="text" class="ion-autocomplete booking-box-text" autocomplete="off" placeholder="Enter Pick Up Point"
               ng-model="model"
               item-value-key="name"
               item-view-value-key="view"
               items-method="getTestItems(query)"
               items-method-value-key="items"
               placeholder="Enter test query ..."
               items-clicked-method="itemsClicked(callback)"
               items-removed-method="itemsRemoved(callback)"
               max-selected-items="3"/>
		  </label>
		</div>
	</div>

	<div class="item item-divider booking-box">
	   <i class="ion-ios-location" style="color:#FF0000;"></i> Drop Point
	</div>
	<div class="item item-text-wrap booking-box">
	  	<div class="item-input-inset booking-box">
	  		<i class="ion-edit"></i>
		  <label class="item-input-wrapper booking-box" style="color:#fff;">
		    <input ion-autocomplete type="text" class="ion-autocomplete booking-box-text" autocomplete="off" placeholder="Enter Drop Point"
               ng-model="model"
               item-value-key="name"
               item-view-value-key="view"
               items-method="getTestItems(query)"
               items-method-value-key="items"
               placeholder="Enter test query ..."
               items-clicked-method="itemsClicked(callback)"
               items-removed-method="itemsRemoved(callback)"
               max-selected-items="3"/>
		  </label>
		</div>
	</div>	

	<div class="row row-bottom col-offset-10">
		<div class="col col-60">
			<button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored ">Estimate Charge
			</button>
		</div>
		<div class="col col-20">
			<button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored ">Book
			</button>
		</div>
	</div>
  </div>	
  </ion-content>
</ion-view>